@article{hartog_spectroscopic_1998,
  title = {Spectroscopic {{Data}} for the 6it s6it p 3 {{P1}} level of {{Lu}}+ for the {{Determination}} of the {{Solar Lutetium Abundance}}},
  volume = {178},
  issn = {0038-0938, 1573-093X},
  doi = {10.1023/A:1005088315480},
  abstract = {We report spectroscopic measurements on the 6s6p 3P1 level of Lu+ at 28503.16 cm-1. The radiative lifetime of this level was measured to be 37.4 \ensuremath{\pm} 1.9 ns using time-resolved laser-induced fluorescence of a slow ion beam. Branching fractions were determined from Lu spectra recorded using the 1.0 m Fourier transform spectrometer at the National Solar Observatory. Thelog(gf) values determined by combining the radiative lifetime and branching fractions for the \ensuremath{\lambda}3507.39,\ensuremath{\lambda} 5983.90, and \ensuremath{\lambda}6221.87 (air wavelengths) lines are - 1.16 \ensuremath{\pm} 0.03, - 1.16 \ensuremath{\pm} 0.06, and -0.76 \ensuremath{\pm} 0.04, respectively. The\ensuremath{\lambda} 6221.87 line has been identified by Bord, Cowley, and Mirijanian (1997) as the best candidate for the determination of the solar lutetium abundance because it is only slightly blended in the solar spectrum. The present \ensuremath{\lambda}6221.87 transition probability measurement brings their solar lutetium abundance into good agreement with the CI chondrite abundance.},
  language = {en},
