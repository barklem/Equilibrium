  issn = {0067-0049},
  doi = {10.1088/0067-0049/182/1/80},
  abstract = {We have derived new abundances of the rare earth elements Pr, Dy, Tm, Yb, and Lu for the solar photosphere and for five very metal-poor, neutron-capture r -process-rich giant stars. The photospheric values for all five elements are in good agreement with meteoritic abundances. For the low-metallicity sample, these abundances have been combined with new Ce abundances from a companion paper, and reconsideration of a few other elements in individual stars, to produce internally consistent Ba, rare earth, and Hf (56 \ensuremath{\leq} Z \ensuremath{\leq} 72) element distributions. These have been used in a critical comparison between stellar and solar r -process abundance mixes.},
  language = {en},
  timestamp = {2015-12-09T10:23:48Z},
  number = {1},
  urldate = {2015-12-09},
  journal = {ApJS},
