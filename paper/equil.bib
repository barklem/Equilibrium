  pages = {850},
  file = {IOP Full Text PDF:/Users/barklem/Library/Application Support/Zotero/Profiles/op0j1739.default/zotero/storage/4SRCGZMA/Lawler et al. - 2004 - Improved Atomic Data for Ho II and New Holmium Abu.pdf:application/pdf}
